<%=render './form' %>
<br>
<img src="../info.JPG" width="100%"/>
<%= form_for :notice, url: notice_path, method: :patch do |f| %>
<table width="100%" border="1" style="font-size:10px">
  <tr>
    <td><%= f.label :date %></td>
    <td><%= f.text_field :date, :style => "width:100%;border:0" %></td>
  </tr>

  <tr>
    <td><%= f.label :subject %></td>
    <td><%= f.text_field :subject, :style => "width:100%;border:0" %></td>
  </tr>

  <tr>
    <td><%= f.label :text %></td>
    <td><%= f.text_area :text, :style => "width:100%;border:0" %></td>
  </tr>
<table>

<table align="center" style="font-size:10px">
  <tr style="text-align:center">
    <td width=40><%=link_to '이전', notice_path %></td>
    <td width=40><%= f.submit '등록'%></td>
  </tr>
</table>
<% end %>
